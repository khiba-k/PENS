datasource db {
    url       = env("DATABASE_URL")
    directUrl = env("DIRECT_URL")
    provider  = "postgresql"
}

generator client {
    provider = "prisma-client-js"
}

model Events {
    id          Int      @id @default(autoincrement())
    createdAt   DateTime @default(now())
    eventName   String
    location    String
    price       Float
    description String
}
